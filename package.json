{
  "name": "video-processing-cli",
  "bin": "src/app.js",
  "private": true,
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "cli-color": "^1.2.0",
    "commander": "~2.9.0",
    "which": "^1.3.0"
  },
  "devDependencies": {
    "jest": "^20.0.1",
    "jest-cli": "^19.0.2",
    "pkg": "~4.2.0"
  },
  "scripts": {
